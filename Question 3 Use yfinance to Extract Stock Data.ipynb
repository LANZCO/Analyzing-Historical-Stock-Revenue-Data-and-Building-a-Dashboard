      "source": [
        "import yfinance as yf\n",
        "\n",
        "# 获取GameStop股票数据\n",
        "gme_data = yf.Ticker(\"GME\")\n",
        "\n",
        "# 获取历史数据\n",
        "gme_data = gme_data.history(period=\"1d\")\n",
        "\n",
        "# 重置索引\n",
        "gme_data.reset_index(inplace=True)\n",
        "\n",
        "# 保存数据\n",
        "gme_data.to_csv(\"gme_data.csv\", index=False)\n",
        "\n",
        "# 显示前五行\n",
        "print(gme_data.head())\n"
      ]
